    "@types/webpack": "^4.0.0",
    "awesome-typescript-loader": "^5.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.0.1",
    "istanbul-instrumenter-loader": "^2.0.0",
